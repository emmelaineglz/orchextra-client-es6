{
  "name": "orchextra-client",
  "version": "3.0.0",
  "description": "Autenticaci√≥n de Clientes y Usuarios de Orchextra-Client",
  "main": "Auth.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && npm run coverage",
    "coverage": "nyc ava",
    "postinstall": "npm run build",
    "build": "babel src/ --out-dir . --presets es2015 --ignore node_modules/*,test/*,coverage/*,dist/*,.nyc_output/* --no-babelrc  --plugins add-module-exports"
  },
  "ava": {
    "require": "babel-register",
    "files": [
      "test/**/*-test.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "test/*",
      "coverage/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text"
    ]
  },
  "author": "Ethel Gonzalez <ethel.gonzalez@gigigo.com.mx>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "bab": "0.0.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "nyc": "^10.0.0",
    "standard": "^8.6.0",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "cupones"
  ]
}
